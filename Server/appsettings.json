{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "MyHttpsEndpoint": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "OpenTelemetry": {
    "IsEnabled": "true"
  },
  "Jaeger": {
    "Enabled": "true",
    "Host": "localhost",
    "Port": "6831"
  }
}
